{
  "compilerOptions": {
   "target": "es2017",
   "module": "commonjs",
   "lib": [
    "es2017",
    "es7",
    "es6",
    "dom"
   ],
   "declaration": true,
   "outDir": "dist",
   "strict": true,
   "esModuleInterop": true
  },
  "exclude": [
   "node_modules",
   "dist"
  ],
  
  "typedocOptions": {
    "out": "docs",
    "name": "MDCDEV.js",
    "themeColor": "#6A329F",

    "entryPoints": [
      "index.ts"
      ],
    //"readme": "none",
    "plugin": ["typedoc-material-theme"],

   // "theme": "minimal" 
   }

 }